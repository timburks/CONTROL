<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" 
"http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>nginx</string>
    <key>OnDemand</key>
    <false/>
    <key>Program</key>
    <string>nginx</string>
    <key>KeepAlive</key>
    <true/>
    <key>LaunchOnlyOnce</key>
    <true/>
    <key>WorkingDirectory</key>
    <string>/xmachine/nginx/sbin</string>
    <key>ProgramArguments</key>
    <array>
	<string>nginx</string>
	<string>-c</string>
	<string>/xmachine/nginx.conf</string>
    </array>
</dict>
</plist>

